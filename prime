#include<stdio.h>
int main() {
int a[100],i,n,b,j;
    printf("enter your range ");
    scanf("%d",&n);
    for(i=0;i<n;i++)
    { 
        scanf("%d",&a[i]);
    }
    for(i=0;i<n;i++)
    { b=0;
        for(j=2;j<a[i];j++)
        { 
        if(a[i]%j==0)
        b++;
        break;
        }
        if(b==0)
        {
            printf("%d ",a[i]);

        }


    } 
    return 0;
    }
