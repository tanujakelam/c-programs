#include <stdio.h>
int max_of_of_four(int a,int b,int c,int d);
int main() {
    int a, b, c, d;
    scanf("%d %d %d %d", &a, &b, &c, &d);
    int ans = max_of_four(a, b, c, d);
    printf("%d", ans);
    
    return 0;
}
int max_of_four(int a,int b,int c,int d)
{
    int x,y;
    x=a>b?a:b;
    y=c>d?c:d;
    if(x>y)
    return x;
    else {
    return y;
    }
}
