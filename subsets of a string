#include<stdio.h>
#include<string.h>
void main()
{  char c[100];
  printf("enter your number");
  scanf("%s",c);
  int n=strlen(c);
  int j=1,i=0,k=0,p;
  while(k<n)
  {
      printf("%c",c[k]);
      printf("\n");
      k++;
      
  }
  while(i<n-1&&j<n)
  { printf("%c",c[i]);
    printf("%c",c[j]);
    printf("\n");
    j++;
    i++;
  } 
  i=0,j=1,p=2;
  while(i<n-2&&j<n-1&&p<n)
  { printf("%c",c[i]);
    printf("%c",c[j]);
    printf("%c",c[p]);
    printf("\n");
    j++;
    i++;
    p++;
  }
  printf("%s",c);
}
